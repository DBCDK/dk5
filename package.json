{
  "name": "tanterne",
  "version": "1.0.0",
  "description": "DK5 Digital",
  "main": "src/main.js",
  "scripts": {
    "dev": "npm run build:dev & . ./env.env && npm run serve",
    "lint": "eslint ./src ./integration --cache",
    "serve": "nodemon --exec node --no-stdin src/main.js",
    "build": "webpack --define process.env.NODE_ENV='\"production\"' --progress --colors -p",
    "build:dev": "webpack --progress --colors --watch -d",
    "precommit": "npm run lint && npm run test && npm run test:integration:local",
    "test": ". ./test.env && mocha --recursive ./src/**/*.test.js --compilers js:babel-register",
    "test:integration": ". ./test.env && npm run build && TEST_ENV=saucelabs wdio ./integration/config/wdio.conf.js",
    "test:integration:local": "rm -rf errorShots/*.png && . ./env.env && . ./test.env && TEST_ENV=local wdio ./integration/config/wdio.conf.js",
    "test:integration:jenkins": "npm run build && TEST_ENV=jenkins wdio ./integration/config/wdio.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/tanterne.git"
  },
  "keywords": [
    "DK5"
  ],
  "author": "DBCDK",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/tanterne/issues"
  },
  "engines": {
    "node": ">=6.7.0"
  },
  "homepage": "https://github.com/DBCDK/tanterne#readme",
  "dependencies": {
    "autocomplete": "^0.0.1",
    "autoprefixer": "^7.1.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.26.0",
    "dbc-node-logger": "^2.0.0",
    "elasticsearch": "^13.3.1",
    "fast-levenshtein": "^2.0.6",
    "koa": "^2.4.0",
    "koa-body": "^2.5.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^3.0.0",
    "newrelic": "^2.3.2",
    "preact": "8.2.6",
    "preact-compat": "3.17.0",
    "preact-enroute": "1.2.3",
    "prop-types": "^15.6.0",
    "react-simpletabs": "^0.7.0",
    "request": "^2.83.0",
    "stdio": "^0.2.7",
    "superagent": "^3.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.3",
    "sinon": "^4.1.1",
    "style-loader": "^0.19.0",
    "wdio-junit-reporter": "^0.3.1",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-sauce-service": "^0.4.4",
    "wdio-selenium-standalone-service": "0.0.8",
    "webdriverio": "^4.6.1",
    "webpack": "^3.8.0"
  }
}
